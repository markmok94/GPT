{
    "breakpointLocation": "NONE",
    "browserLog": false,
    "closeCookieModals": false,
    "debugLog": false,
    "downloadCss": true,
    "downloadMedia": true,
    "excludes": [
        {
            "glob": "/**/*.{png,jpg,jpeg,pdf}"
        }
    ],
    "globs": [
        {
            "glob": "https://crawlee.dev/js/*/*"
        }
    ],
    "headless": true,
    "ignoreCorsAndCsp": true,
    "ignoreSslErrors": false,
    "injectJQuery": false,
    "keepUrlFragments": false,
    "linkSelector": "a[href]",
    "pageFunction": "async function pageFunction(context) {\n    const name = context.request.userData.name;\n    let currentSupply = null;\n    const label = Array.from(document.querySelectorAll('div')).find(el => el.textContent.trim() === 'Current Supply');\n    if (label) {\n        const valueDiv = label.parentElement.nextElementSibling || label.nextElementSibling;\n        if (valueDiv) {\n            const val = valueDiv.querySelector('div');\n            currentSupply = (val || valueDiv).textContent.trim();\n        }\n    }\n    context.log.info(`URL: ${context.request.url}, Current Supply: ${currentSupply}`);\n    return { name, url: context.request.url, currentSupply };\n}",
    "postNavigationHooks": "[\n    async (crawlingContext) => {\n        // ...\n    }\n]",
    "preNavigationHooks": "[\n    async (crawlingContext, gotoOptions) => {\n        // ...\n    }\n]",
    "proxyConfiguration": {
        "useApifyProxy": true
    },
    "respectRobotsTxtFile": true,
    "runMode": "DEVELOPMENT",
