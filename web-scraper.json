{
    "breakpointLocation": "NONE",
    "browserLog": false,
    "closeCookieModals": false,
    "debugLog": false,
    "downloadCss": true,
    "downloadMedia": true,
    "excludes": [
        {
            "glob": "/**/*.{png,jpg,jpeg,pdf}"
        }
    ],
    "globs": [
        {
            "glob": "https://crawlee.dev/js/*/*"
        }
    ],
    "headless": true,
    "ignoreCorsAndCsp": false,
    "ignoreSslErrors": false,
    "injectJQuery": true,
    "keepUrlFragments": false,
    "linkSelector": "a[href]",
    "pageFunction": "async function pageFunction(context) {\n    const name = context.request.userData.name;\n    const xpath = \"//*[text()='Current Supply']/following::div[1]\";\n    const element = document.evaluate(xpath, document, null, XPathResult.FIRST_ORDERED_NODE_TYPE, null).singleNodeValue;\n    const currentSupply = element ? element.textContent.trim() : null;\n    context.log.info(`URL: ${context.request.url}, Current Supply: ${currentSupply}`);\n    return { name, url: context.request.url, currentSupply };\n}",
    "postNavigationHooks": "[\n    async (crawlingContext) => {\n        // ...\n    }\n]",
    "preNavigationHooks": "[\n    async (crawlingContext, gotoOptions) => {\n        // ...\n    }\n]",
    "proxyConfiguration": {
        "useApifyProxy": true
    },
    "respectRobotsTxtFile": true,
